{
	"name": "Interpres",
	"version": "1.0",
	"description": "Select text to translate.",
	"icons": {
		"16": "images/to_16.png",
		"30": "images/to_30.png",
		"40": "images/to_40.png",
		"80": "images/to_80.png"
	},
	"background": {
		"scripts": ["background.js", "jquery-3.4.1.min.js"],
		"persistent": false
	},
	"content_scripts": [{
		"matches": [ "<all_urls>" ],
		"all_frames": true,
		"js": [
			"jquery-3.4.1.min.js",
			"xregexp-all.js",
			"contentScript.js"
		]
	}],
	"browser_action": {
		"default_icon": "images/to_40.png"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"web_accessible_resources": [
		"trans_popup.html",
		"trans_popup.js"
	],
	"manifest_version": 2
}
