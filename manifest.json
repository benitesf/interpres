{
	"name": "Interpres",
	"version": "1.0",
	"description": "Select text to translate.",
	"background": {
		"scripts": ["background.js", "jquery-3.4.1.min.js"],
		"persistent": false
	},
	"content_scripts": [{
		"matches": [ "<all_urls>" ],
		"all_frames": true,
		"js": [
			"jquery-3.4.1.min.js",
			"xregexp-all.js",
			"contentScript.js"
		]
	}],
	"browser_action": {
		"default_icon": "images/eutoes_icon.png"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"web_accessible_resources": [
		"popup.js"
	],
	"manifest_version": 2
}
